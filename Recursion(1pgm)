package intro;
import java.util.*;
public class recurssion {
	public static void printnum(int n) {
		if(n==0) {
			return;
		}
		System.out.println(n);
		printnum(n-1);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		printnum(n);
		
	}

}
